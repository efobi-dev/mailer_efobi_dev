{
	"name": "@efobi/mailer",
	"version": "4.0.0",
	"description": "A TypeScript SDK for Emails",
	"keywords": [
		"emails",
		"sdk",
		"typescript",
		"api",
		"service"
	],
	"homepage": "https://github.com/efobi-dev/mail_efobi_dev",
	"bugs": {
		"url": "https://github.com/efobi-dev/mail_efobi_dev/issues"
	},
	"author": {
